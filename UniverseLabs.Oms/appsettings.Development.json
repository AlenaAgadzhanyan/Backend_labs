{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "RabbitMqSettings": {
    "HostName": "rabbitmq",
    "Port": 5672,
    "OrderCreated": {
      "Queue": "oms.order.created",
      "BatchSize": 10,
      "BatchTimeoutSeconds": 5,
      "DeadLetter": {
        "Dlx": "oms.order.created.dlx",
        "Dlq": "oms.order.created.dlq",
        "RoutingKey": "oms.order.created.dl"
      }
    },
    "OrderStatusChanged": {
      "Queue": "oms.order.status.changed",
      "BatchSize": 10,
      "BatchTimeoutSeconds": 5,
      "DeadLetter": {
        "Dlx": "oms.order.status.changed.dlx",
        "Dlq": "oms.order.status.changed.dlq",
        "RoutingKey": "oms.order.status.changed.dl"
      }
    }
  },
  "HttpClient": {
    "Oms": {
      "BaseAddress": "http://oms-api:8080"
    }
  },
  "AllowedHosts": "*"
}
